<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.ommu.inlisjogja">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".TrackActivity"
            android:label="@string/title_track">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_search"></activity>
        <activity
            android:name=".SearchAdvancedActivity"
            android:label="@string/title_search_advance"></activity>
        <activity
            android:name=".TrackMemberActivity"
            android:label="@string/title_track"></activity>
    </application>

</manifest>
